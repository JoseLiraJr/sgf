unit uFuncionario;

interface

uses
  Data.DB, System.SysUtils;

type
  TFuncionario = class
  public

    constructor Create;
    destructor Destroy; override;
  end;

implementation

{ TFuncionario }

constructor TFuncionario.Create;
begin
  inherited Create;
end;

destructor TFuncionario.Destroy;
begin
  inherited Destroy;
end;

end.
